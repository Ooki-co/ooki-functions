{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc -w",
    "emulator": "firebase emulators:start --only functions --inspect-functions",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy:dev": "firebase deploy --only functions:$npm_config_function --project ooki-dev",
    "deploy:prod": "firebase deploy --only functions:$npm_config_function --project ooki-prod",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/index.js",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "helmet": "^4.4.1",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0",
    "node": "^12.20.0",
    "typescript": "^4.1.3"
  },
  "private": true
}
